{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ]
    },
    // "jsx": "react-jsx",
    // "jsxImportSource": "preact"
  }
}